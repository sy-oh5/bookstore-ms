      "sourceRoot": "apps/rental-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/rental-service/tsconfig.app.json"
      }
    },
    "book-service": {
      "type": "application",
      "root": "apps/book-service",
      "entryFile": "main",
      "sourceRoot": "apps/book-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/book-service/tsconfig.app.json"
      }
    },
    "api-gateway": {
      "type": "application",
      "root": "apps/api-gateway",
      "entryFile": "main",
      "sourceRoot": "apps/api-gateway/src",
      "compilerOptions": {
        "tsConfigPath": "apps/api-gateway/tsconfig.app.json"
      }
    }
  }
